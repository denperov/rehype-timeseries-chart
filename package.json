{
  "author": "Denis Perov",
  "name": "rehype-timeseries-chart",
  "version": "0.0.10",
  "keywords": [
    "rehype",
    "plugin",
    "markdown",
    "svg",
    "chart",
    "timeseries",
    "csv"
  ],
  "description": "Rehype plugin to render CSV code blocks as SVG timeseries charts",
  "repository": {
    "type": "git",
    "url": "https://github.com/denperov/rehype-timeseries-chart.git"
  },
  "bugs": "https://github.com/denperov/rehype-timeseries-chart/issues",
  "license": "MIT",
  "type": "module",
  "files": ["dist"],
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup && type-coverage",
